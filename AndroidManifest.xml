        <!-- *************************receiver************************ -->
        <receiver android:name=".receiver.BootNetReceiver" >
            <intent-filter android:priority="1000" >
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>
        <receiver android:name=".receiver.AllReceiver" >
            android:priority="1000" >
            <intent-filter>

                <!-- 手机唤醒解锁 -->
                <action android:name="android.intent.action.USER_PRESENT" />
                <!-- 关闭屏幕 -->
                <action android:name="android.intent.action.CLOSE_SYSTEM_DIALOGS" />
                <!-- 电话相关权限 -->
                <action android:name="android.intent.action.NEW_OUTGOING_CALL" />
                <!-- 系统壁纸更换 -->
                <action android:name="android.intent.action.WALLPAPER_CHANGED" />
                <!-- 设备进入 USB 大容量存储模式 -->
                <action android:name="android.intent.action.UMS_CONNECTED" />
                <!-- 设备从 USB 大容量存储模式退出。 -->
                <action android:name="android.intent.action.UMS_DISCONNECTED" />
                <!-- 屏幕已经被打开 3.0以下有效 -->
                <action android:name="android.intent.action.SCREEN_ON" />
                <!-- 屏幕已经被关闭 3.0以下有效 -->
                <action android:name="android.intent.action.SCREEN_OFF" />
                <!-- 充电状态，或者电池的电量发生变化。 -->
                <action android:name="android.intent.action.BATTERY_CHANGED" />
                <!-- 表示电池电量低。 -->
                <action android:name="android.intent.action.BATTERY_LOW" />
                <!-- 表示电池电量充足，即从电池电量低变化到饱满时会发出广播 -->
                <action android:name="android.intent.action.BATTERY_OKAY" />
                <!-- 插上外部电源时发出的广播 -->
                <action android:name="android.intent.action.ACTION_POWER_CONNECTED" />
                <!-- 已断开外部电源连接时发出的广播 -->
                <action android:name="android.intent.action.ACTION_POWER_DISCONNECTED" />
                <!-- 设备的配置信息已经改变，参见 Resources.Configuration. -->
                <action android:name="android.intent.action.CONFIGURATION_CHANGED" />
                <!-- 在耳机口上插入耳机时发出的广播 -->
                <action android:name="android.intent.action.HEADSET_PLUG" />
                <!-- 改变输入法时发出的广播 -->
                <action android:name="android.intent.action.INPUT_METHOD_CHANGED" />
                <!-- 移动APP完成之后，发出的广播(移动是指:APP2SD) -->
                <action android:name="android.intent.action.EXTERNAL_APPLICATIONS_AVAILABLE" />
                <!-- 正在移动APP时，发出的广播(移动是指:APP2SD) -->
                <action android:name="android.intent.action.EXTERNAL_APPLICATIONS_UNAVAILABLE" />
                <!-- 按下照相时的拍照按键(硬件按键)时发出的广播 -->
                <action android:name="android.intent.action.ACTION_CAMERA_BUTTON" />
                <!-- 关闭或打开飞行模式时的广播 -->
                <action android:name="android.intent.action.AIRPLANE_MODE" />
                <!-- 屏日期被改变。 -->
                <action android:name="android.intent.action.DATE_CHANGED" />
                <!-- 用户按下了“Media Button -->
                <action android:name="android.intent.action.MEDIA_BUTTON" />
                <!-- 扩展介质（扩展卡）已经从 SD 卡插槽拔出，但是挂载点 (mount point) 还没解除 (unmount)。 -->
                <action android:name="android.intent.action.MEDIA_BAD_REMOVAL" />
                <!-- 用户想要移除扩展介质（拔掉扩展卡）。 -->
                <action android:name="android.intent.action.MEDIA_EJECT" />
                <!-- 扩展介质被插入，而且已经被挂载。 -->
                <action android:name="android.intent.action.MEDIA_MOUNTED" />
                <!-- 扩展介质被移除。 -->
                <action android:name="android.intent.action.MEDIA_REMOVED" />
                <!-- 已经扫描完介质的一个目录。 -->
                <action android:name="android.intent.action.MEDIA_SCANNER_FINISHED" />
                <!-- 扩展介质的挂载被解除 (unmount)，因为它已经作为 USB 大容量存储被共享。 -->
                <action android:name="android.intent.action.MEDIA_SHARED" />
                <!-- 扩展介质存在，但是还没有被挂载 (mount)。 -->
                <action android:name="android.intent.action.MEDIA_UNMOUNTED" />
                <!-- 更新将要（真正）被安装。 -->
                <action android:name="android.intent.action.PROVIDER_CHANGED" />
            </intent-filter>
        </receiver>
    </application>

</manifest>